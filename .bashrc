# User specific aliases and functions
a

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

alias ll='ls -alFh'

alias rs='rsync --archive --verbose --progress --human-readable --whole-file'
alias rsd='rsync --archive --verbose --progress --human-readable --whole-file --delete'

alias du1='du -sh *'
alias du.='du -h . -d 1'

alias x='xdg-open .'
