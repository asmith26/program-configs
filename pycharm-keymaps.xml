-<!-- Copy to ~/.PyCharm*/config/keymaps/my-keymaps.xml -->
<keymap version="1" name="my-keymap" parent="Default for XWin">
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateFindToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
  </action>
  <action id="ActivatePythonConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt 4" />
  </action>
  <action id="ActivateRunToolWindow">
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="ActivateStructureToolWindow" />
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt 5" />
  </action>
  <action id="AutoIndentLines" />
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
  </action>
  <action id="ChangesView.UnshelveSilently" />
  <action id="CloseActiveTab" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Console.Execute.Multiline" />
  <action id="DebugClass">
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EditorSplitLine" />
  <action id="EditorStartNewLineBefore" />
  <action id="ExecuteInPyConsoleAction">
    <keyboard-shortcut first-keystroke="shift alt e" />
    <keyboard-shortcut first-keystroke="ctrl enter" />
  </action>
  <action id="FindUsages">
    <keyboard-shortcut first-keystroke="alt f7" />
    <keyboard-shortcut first-keystroke="ctrl u" />
  </action>
  <action id="ForceStepInto">
    <keyboard-shortcut first-keystroke="ctrl alt i" />
  </action>
  <action id="GotoImplementation">
    <mouse-shortcut keystroke="control alt button1" />
  </action>
  <action id="GotoSuperMethod" />
  <action id="ImplementMethods" />
  <action id="IpnbRunCellInplaceAction" />
  <action id="OptimizeImports" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl i" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="RunClass">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
    <keyboard-shortcut first-keystroke="alt enter" />
  </action>
  <action id="SaveAll" />
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="ctrl s" />
  </action>
  <action id="StepOut">
    <keyboard-shortcut first-keystroke="ctrl alt u" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="ctrl alt o" />
  </action>
  <action id="Stop">
    <keyboard-shortcut first-keystroke="ctrl f2" />
    <keyboard-shortcut first-keystroke="shift ctrl alt enter" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="ViewSource" />
  <action id="editRunConfigurations">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
</keymap>
